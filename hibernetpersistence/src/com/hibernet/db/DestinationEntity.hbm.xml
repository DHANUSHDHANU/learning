<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hibernet.db.DestinationEntity" table="destination" schema="cimbidia">
        <id name="id" column="id"/>
        <property name="shipto" column="shipto"/>
        <property name="streetaddress" column="streetaddress"/>
        <property name="city" column="city"/>
        <property name="state" column="state"/>
        <property name="country" column="country"/>
        <property name="deliverystart" column="deliverystart"/>
        <property name="deliveryend" column="deliveryend"/>
        <property name="supplier" column="supplier"/>
    </class>
</hibernate-mapping>